  },
  "homepage": "https://github.com/NonsoAmadi10/Rule-Validation-Api#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "express": "^4.17.1",
    "underscore-contrib": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
